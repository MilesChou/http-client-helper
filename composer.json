{
    "name": "mileschou/mocker",
    "description": "The mock helper for HTTP client.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http-client"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "mileschou/psr": "^1.1"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Require ^6 if need use Guzzle MockerBuilder",
        "php-http/guzzle6-adapter": "Require ^2 if need use Guzzle with PSR-18 adapter"
    },
    "require-dev": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.1",
        "php-http/guzzle6-adapter": "^2.0",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Mocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
