{
    "name": "mileschou/mocker",
    "description": "The mock helper for HTTP client.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http-client"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^7.5 | ^8 | ^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Mocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
